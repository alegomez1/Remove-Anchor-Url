function removeAnchor(str) {

    for (let i = 0; i < str.length; i++) {
        let letter = str[i];
        let positon = str[0];



        if (letter === '#'){
            positon = i;

            str = str.slice(0, positon)

        }


    }

    console.log(str);
}

removeAnchor('www.apple.com#/iphone');


